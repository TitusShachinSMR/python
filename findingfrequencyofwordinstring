a=input("enter the string ")
c=a.strip()
c=c.replace(',',' ')
c=c.replace('.',' ' )
c=c.split(' ')
b=input("enter the word frequency to find")
n=len(c)
l=0
for i in c:
    if(i==b):
        l=l+1
    
print(f"frequency of word:{l}")
