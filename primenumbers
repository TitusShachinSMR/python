import sys
def prime(a):
  if(a==1):
    print("it is neither prime nor composite")
    sys.exit(0)
  if(a==2):
    print("it is prime")
    sys.exit(0)
  if(a>=3):
    for i in range(3,a):
      if(a%i==0 and a!=i):
        print("it is composite")
        sys.exit(0)
  print("it is prime number")
    
    

b=input("enter the number")
a=int(b)
prime(a)
